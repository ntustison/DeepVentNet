	Journal = {Neurobiol Aging},
	Journal-Full = {Neurobiology of aging},
	Keywords = {Amyloidosis; Cerebrovascular disease; Cognition; Mild cognitive impairment; Primary age-related tauopathy; Suspected non-AD pathology},

	Mesh = {Algorithms; Analysis of Variance; Artificial Intelligence; Female; Frontal Lobe; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Reference Standards; Sensitivity and Specificity; Software; Temporal Lobe},
	Month = {Jul},
	Number = {4},

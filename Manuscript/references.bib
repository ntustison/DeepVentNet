	Date-Modified = {2016-07-31 01:28:27 +0000},
	Doi = {10.1016/j.neuroimage.2009.04.024},
	Institution = {Neuroimaging Research Branch, National Institute on Drug Abuse, National Institutes of Health, Baltimore, MD, USA. gengx@nida.nih.gov},

	Mesh = {Adolescent; Age of Onset; Alcoholism; Cerebral Cortex; Female; Frontal Lobe; Health Status; Humans; Magnetic Resonance Imaging; Male; Marijuana Abuse; Mental Health; Parietal Lobe; Prospective Studies; Recovery of Function; Socioeconomic Factors; Young Adult},
	Month = {Dec},

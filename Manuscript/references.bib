	Journal = {Hum Brain Mapp},
	Journal-Full = {Human brain mapping},
	Keywords = {Alzheimer's disease; Brodmann area 35; biomarker; cornu ammonis; entorhinal cortex; hippocampus; magnetic resonance imaging; perirhinal cortex; segmentation},
	Mesh = {Algorithms; Automatic Data Processing; Brain Mapping; Cognitive Dysfunction; Female; Hippocampus; Humans; Image Processing, Computer-Assisted; Learning; Magnetic Resonance Imaging; Male; Temporal Lobe},
	Month = {Jan},
	Number = {1},

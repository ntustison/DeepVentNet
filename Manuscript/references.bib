	Number = {9},
	Pages = {e45081},
	Pmc = {PMC3445568},
	Pmid = {23028771},
	Pst = {ppublish},
	Title = {Accuracy and reliability of automated gray matter segmentation pathways on real and simulated structural magnetic resonance images of the human brain},
	Volume = {7},

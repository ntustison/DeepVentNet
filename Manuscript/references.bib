	Title = {Quantitative in vivo measurement of gyrification in the human brain: changes associated with aging},
	Volume = {9},
	Year = {1999}}

@article{makris2008,
	Abstract = {The structural effects of cocaine on neural systems mediating cognition and motivation are not well known. By comparing the thickness of neocortical and paralimbic brain regions between cocaine-dependent and matched control subjects, we found that four of 18 a priori regions involved with executive regulation of reward and attention were significantly thinner in addicts. Correlations were significant between thinner prefrontal cortex and reduced keypresses during judgment and decision making of relative preference in addicts, suggesting one basis for restricted behavioral repertoires in drug dependence. Reduced effortful attention performance in addicts also correlated with thinner paralimbic cortices. Some thickness differences in addicts were correlated with cocaine use independent of nicotine and alcohol, but addicts also showed diminished thickness heterogeneity and altered hemispheric thickness asymmetry. These observations suggest that brain structure abnormalities in addicts are related in part to drug use and in part to predisposition toward addiction.},
	Author = {Makris, Nikos and Gasic, Gregory P and Kennedy, David N and Hodge, Steven M and Kaiser, Jonathan R and Lee, Myung Joo and Kim, Byoung Woo and Blood, Anne J and Evins, A Eden and Seidman, Larry J and Iosifescu, Dan V and Lee, Sang and Baxter, Claudia and Perlis, Roy H and Smoller, Jordan W and Fava, Maurizio and Breiter, Hans C},

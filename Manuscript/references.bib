	Keywords = {angular gyrus; combinatorial semantics; compositionality; conceptual combination; semantic integration; semantic memory},
	Mesh = {Adult; Aged; Association Learning; Brain Mapping; Comprehension; Dementia; Female; Humans; Image Processing, Computer-Assisted; Individuality; Magnetic Resonance Imaging; Male; Middle Aged; Oxygen; Parietal Lobe; Semantics; Young Adult},

	Doi = {10.1016/j.neuroimage.2011.11.012},
	Journal = {Neuroimage},
	Journal-Full = {NeuroImage},
	Mesh = {Aged; Brain Mapping; Cerebral Cortex; Female; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Male},
	Month = {Feb},
	Number = {4},
	Pages = {3805-20},
	Pmc = {PMC3288171},
	Pmid = {22119005},

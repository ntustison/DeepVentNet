	Journal = {Neurology},
	Journal-Full = {Neurology},
	Mesh = {Aged; Aged, 80 and over; Alzheimer Disease; Apolipoproteins E; Atrophy; Brain; Cognitive Dysfunction; Databases, Factual; Disease Progression; Female; Humans; Linear Models; Longitudinal Studies; Magnetic Resonance Imaging; Male; Statistics, Nonparametric; Time Factors},

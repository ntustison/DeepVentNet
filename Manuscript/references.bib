	Doi = {10.1093/cercor/bhn113},
	Journal = {Cereb Cortex},
	Journal-Full = {Cerebral cortex (New York, N.Y. : 1991)},
	Mesh = {Aged; Aged, 80 and over; Alzheimer Disease; Amyloid; Brain Mapping; Cerebral Cortex; Dementia; Female; Humans; Male; Middle Aged; Plaque, Amyloid; Severity of Illness Index},
	Month = {Mar},

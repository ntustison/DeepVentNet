	Keywords = {Cerebral cortex; Cortical surface area; Cortical thickness; Structural MRI; Typical development},
	Mesh = {Adolescent; Cerebral Cortex; Child; Female; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Male; Organ Size; Sex Characteristics; Young Adult},
	Month = {Feb},

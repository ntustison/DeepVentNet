	Doi = {10.1017/S135561771000072X},
	Journal = {J Int Neuropsychol Soc},
	Journal-Full = {Journal of the International Neuropsychological Society : JINS},
	Mesh = {Aged; Brain Mapping; Entorhinal Cortex; Female; Functional Laterality; Geriatric Assessment; Humans; Magnetic Resonance Imaging; Male; Memory Disorders; Middle Aged; Neuropsychological Tests; Reproducibility of Results},
	Month = {Sep},

	Journal = {J Am Acad Child Adolesc Psychiatry},
	Journal-Full = {Journal of the American Academy of Child and Adolescent Psychiatry},
	Keywords = {MRI; cortical thickness; music},
	Mesh = {Adolescent; Adolescent Development; Brain; Cerebral Cortex; Child; Health Promotion; Humans; Magnetic Resonance Imaging; Mental Disorders; Music},
	Month = {Nov},
	Number = {11},
	Pages = {1153-61, 1161.e1-2},
	Pmc = {PMC4254594},
